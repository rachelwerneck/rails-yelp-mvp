<%= form_with(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone_number %>
  <%= f.number_field :phone_number %>
   <%= f.label :category %>
  <%= f.number_field :category %>
  <%= f.submit %>
<% end %>
